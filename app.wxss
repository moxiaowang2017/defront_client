@import 'style/weui.wxss';
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 



.image_container {
  position: absolute;
  z-index: -100;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
}

.image_container > .img {
  height: 100%;
  width: 100%;
}
